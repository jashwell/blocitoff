<h1><%= @list.title %></h1>
<div class="col-md-4">
  <% if @list.nil? %>
  <%= link_to "New To-do List", new_list_path, class: 'btn btn-success' %>
  <% end %>
  <%= render partial: 'items/item', locals: { list: @list, item: @item } %>

  <%= render partial: 'items/form', locals: { list: @list } %>
</div>
<div class="col-md-8">

      <%= link_to "Edit List", edit_list_path, class: 'btn btn-success' %>

    <%= link_to "Delete List", @list, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete your list?' } %>
</div>







